{
    "name": "joaovdiasb/laravel-multi-tenancy",
    "description": "Laravel multi-tenancy package",
    "type": "library",
    "license": "MIT",
    "keywords": [
		"laravel",
        "saas",
		"multi-tenancy",
        "multi-tenant",
		"tenancy"
	],
    "authors": [
        {
            "name": "Jo√£o Victor Dias Bittencourt",
            "email": "j.v_dias@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable":true,
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "illuminate/support": "^7.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.0",
        "orchestra/database": "^5.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "9.4",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Joaovdiasb\\LaravelMultiTenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joaovdiasb\\LaravelMultiTenancy\\Tests\\":"tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joaovdiasb\\LaravelMultiTenancy\\LaravelMultiTenancyServiceProvider"
            ],
            "aliases": {
                "Joaovdiasb": "Joaovdiasb\\LaravelMultiTenancy\\LaravelMultiTenancyFacade"
            }
        }
    }
}
